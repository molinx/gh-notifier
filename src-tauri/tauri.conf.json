{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "Github Notifier",
  "version": "0.1.0",
  "identifier": "com.molinx.gh-notifier",
  "mainBinaryName": "Github Notifier",
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
